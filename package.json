{
  "name": "@openapi-ts/validator",
  "version": "1.0.1",
  "description": "OpenAPI validator helpers with TypeScript typings",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
    "lint": "eslint src --ext js,ts --max-warnings 0",
    "prepublish": "yarn clean && yarn build"
  },
  "repository": "git@github.com:henhal/openapi-ts-validator.git",
  "keywords": [
    "OpenAPI",
    "request",
    "response",
    "TypeScript",
    "backend",
    "validation",
    "schema"
  ],
  "author": "Henrik Hall",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "ajv": "^8.5.0",
    "ajv-formats": "^2.1.0",
    "json-schema-ref-parser": "^9.0.7",
    "openapi-types": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "typescript": "^4.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
